{
  "name": "lfds",
  "description": "",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run copy && npm run develop",
    "build": "npm run clean && npm run copy && gatsby build",
    "develop": "gatsby develop",
    "clean": "rm -rf .cache public",
    "serve": "gatsby serve",
    "format": "prettier -w \"{gatsby-*.js,src/**/*.js}\" && stylelint src/**/*.css --fix",
    "test": "prettier -c \"{gatsby-*.js,src/**/*.js}\" && stylelint src/**/*.css",
    "copy": "npm run copy:status && npm run copy:docs && npm run copy:icons",
    "copy:docs": "copyfiles -f \"node_modules/@lansforsakringar/components/dist/docs/main.umd.js\" static/docs",
    "copy:icons": "copyfiles -u 4 \"node_modules/@lansforsakringar/icons/dist/@(svg|sprite)/**/**/*\" static/lf-icons",
    "copy:status": "copyfiles -f \"node_modules/@lansforsakringar/components/dist/docs/data/componentsStatus.json\" src/data"
  },
  "engines": {
    "node": "16.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LF-digitala-kanaler/LFDS"
  },
  "dependencies": {
    "@lansforsakringar/fonts": "^1.0.3",
    "@reach/router": "^1.3.4",
    "@reach/tabs": "^0.17.0",
    "babel-preset-gatsby": "^1.14.0",
    "dangerously-set-html-content": "^1.0.5",
    "date-fns": "^2.17.0",
    "downshift": "^6.1.0",
    "gatsby": "^3.14.6",
    "gatsby-plugin-anchor-links": "^1.2.1",
    "gatsby-plugin-catch-links": "^3.14.0",
    "gatsby-plugin-flexsearch": "^1.0.3",
    "gatsby-plugin-image": "1.14.2",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-netlify": "^3.14.0",
    "gatsby-plugin-netlify-cms": "^5.14.0",
    "gatsby-plugin-offline": "^4.14.0",
    "gatsby-plugin-postcss": "^4.14.0",
    "gatsby-plugin-react-helmet": "^4.2.0",
    "gatsby-plugin-remove-trailing-slashes": "^3.14.0",
    "gatsby-plugin-sharp": "^3.14.0",
    "gatsby-remark-absolute-link-catch": "0.0.1",
    "gatsby-remark-autolink-headers": "^4.2.0",
    "gatsby-remark-component": "^1.1.3",
    "gatsby-remark-copy-linked-files": "^4.2.0",
