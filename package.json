    "react-simple-flex-grid": "^1.3.21",
    "react-syntax-highlighter": "^15.4.3",
    "rehype-react": "^6.1.0",
    "rollup": "^2.78.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "devDependencies": {
    "@lansforsakringar/components": "^10.1.6",
    "@lansforsakringar/icons": "^2.2.3",
    "babel-plugin-prismjs": "^2.1.0",
    "copyfiles": "^2.4.1",
    "fs-extra": "^10.0.0",
    "gh-pages": "^3.1.0",
    "is-relative-url": "^3.0.0",
    "prettier": "^2.5.1",
    "react-device-detect": "^1.15.0",
